{
	"name": "shopify-remake01",
	"version": "0.0.1",
	"scripts": {
		"start": "nova .",
		"dev": "pnpm uninstall svench; svelte-kit dev",
		"build": "svelte-kit build",
		"preview": "svelte-kit preview",
		"theme-watch": "theme watch --dir theme",
		"section-proxy": "shopify-section-proxy --port=$SHOPIFY_SECTION_PROXY_PORT --url=$SHOPIFY_PREVIEW_URL",
		"generate-icons": "fish --command 'node scripts/feather-icons src/lib/feather && touch src/lib/feather/*; node scripts/polaris-icons src/lib/polaris && touch src/lib/polaris/*'",
		"generate-storefront-types": "node scripts/generate-storefront-types",
		"expose-metafields": "node scripts/expose-metafields",
		"svench": "pnpm i -D svench; svench"
	},
	"devDependencies": {
		"@shopify/polaris-icons": "^4.8.0",
		"@sveltejs/adapter-netlify": "next",
		"@sveltejs/adapter-node": "next",
		"@sveltejs/adapter-static": "next",
		"@sveltejs/kit": "next",
		"@tomatrow/zen-format": "^1.0.2",
		"@types/node-fetch": "^2.5.12",
		"autoprefixer": "^10.3.5",
		"cssnano": "^4.1.11",
		"env-cmd": "^10.1.0",
		"lodash": "^4.17.21",
		"node-fetch": "^2.6.5",
		"optional-default-svelte-actions": "link:/Users/ajcaldwell/src/optional-default-svelte-actions",
		"postcss": "^8.3.7",
		"postcss-cli": "^8.3.1",
		"postcss-load-config": "^3.1.0",
		"postcss-nested": "^5.0.6",
		"prettier-plugin-svelte": "^2.4.0",
		"sass": "^1.42.1",
		"svelte": "^3.43.0",
		"svelte-carousel": "^1.0.15",
		"svelte-check": "^2.2.6",
		"svelte-preprocess": "^4.9.5",
		"svelte-typed-context": "^1.0.1",
		"svench": "^0.2.0-31",
		"tailwindcss": "^2.2.15",
		"tslib": "^2.3.1",
		"typescript": "^4.4.3"
	},
	"type": "module",
	"engines": {
		"node": ">= 12.17.0"
	}
}
